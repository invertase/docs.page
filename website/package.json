{
  "private": true,
  "version": "0.0.0",
  "name": "docs.page",
  "scripts": {
    "start": "remix-serve server/build",
    "build": "yarn build:css && remix build",
    "build:css": "tailwindcss -o ./app/styles/tailwind.css --minify",
    "dev": "PORT=3001 ./node_modules/.bin/concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -o ./app/styles/tailwind.css --watch",
    "dev:msw": "MSW_ENABLED=1 yarn dev",
    "postinstall": "remix setup node",
    "test": "yarn rimraf tests/screenshots/* && MSW_ENABLED=1 playwright test",
    "test:headed": "yarn rimraf tests/screenshots/* && MSW_ENABLED=1 playwright test --headed"
  },
  "dependencies": {
    "@code-hike/mdx": "0.3.0-next.20",
    "@docs.page/client": "^1.0.4",
    "@docs.page/server": "^1.0.1",
    "@docsearch/react": "^3.0.0-alpha.42",
    "@heroicons/react": "^1.0.5",
    "@ltd/j-toml": "^1.30.0",
    "@playwright/test": "^1.17.1",
    "@remix-run/express": "^1.1.1",
    "@remix-run/react": "^1.0.6",
    "@remix-run/serve": "^1.0.6",
    "@remix-run/vercel": "^1.0.6",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "classnames": "^2.3.1",
    "color": "^4.1.0",
    "compression": "^1.7.4",
    "country-flag-icons": "^1.4.19",
    "cross-env": "^7.0.3",
    "detect-browser": "^5.3.0",
    "express": "^4.17.1",
    "lodash.get": "^4.4.2",
    "morgan": "^1.10.0",
    "nprogress": "^0.2.0",
    "react": "^17.0.2",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "^17.0.2",
    "react-medium-image-zoom": "^4.3.5",
    "react-router-dom": "^6.2.1",
    "remix": "^1.2.3",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.17.1",
    "@remix-run/dev": "^1.2.3",
    "@tailwindcss/typography": "^0.5.0-alpha.3",
    "@types/color": "^3.0.2",
    "@types/lodash.get": "^4.4.6",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.24",
    "@types/react-copy-to-clipboard": "^5.0.2",
    "@types/react-dom": "^17.0.9",
    "@types/wicg-file-system-access": "^2020.9.4",
    "concurrently": "^6.4.0",
    "msw": "^0.36.3",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.0",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
