{
  "private": true,
  "version": "0.0.0",
  "name": "docs.page",
  "scripts": {
    "start": "remix-serve server/build",
    "build": "yarn build:css && remix build",
    "build:css": "tailwindcss -o ./app/styles/tailwind.css --minify",
    "dev": "PORT=3001 ./node_modules/.bin/concurrently \"npm run dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -o ./app/styles/tailwind.css --watch",
    "dev:msw": "MSW_ENABLED=1 yarn dev",
    "postinstall": "remix setup node",
    "test": "yarn rimraf tests/screenshots/* && MSW_ENABLED=1 playwright test",
    "test:headed": "yarn rimraf tests/screenshots/* && MSW_ENABLED=1 playwright test --headed"
  },
  "dependencies": {
    "@code-hike/mdx": "^0.7.2",
    "@docs.page/client": "^1.0.4",
    "@docs.page/server": "^1.0.1",
    "@docsearch/react": "^3.1.0",
    "@heroicons/react": "^1.0.6",
    "@playwright/test": "^1.22.2",
    "@remix-run/express": "^1.6.1",
    "@remix-run/react": "^1.6.1",
    "@remix-run/serve": "^1.6.1",
    "@remix-run/vercel": "^1.6.1",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@u-wave/react-vimeo": "^0.9.8",
    "classnames": "^2.3.1",
    "color": "^4.2.3",
    "compression": "^1.7.4",
    "country-flag-icons": "^1.5.5",
    "cross-env": "^7.0.3",
    "detect-browser": "^5.3.0",
    "express": "^4.18.1",
    "lodash.get": "^4.4.2",
    "morgan": "^1.10.0",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-medium-image-zoom": "^5.0.1",
    "react-router-dom": "^6.3.0",
    "remix": "^1.6.1",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.22.2",
    "@remix-run/dev": "^1.6.1",
    "@tailwindcss/typography": "^0.5.2",
    "@types/color": "^3.0.3",
    "@types/lodash.get": "^4.4.7",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.14",
    "@types/react-copy-to-clipboard": "^5.0.2",
    "@types/react-dom": "^18.0.5",
    "@types/react-helmet": "^6.1.5",
    "@types/wicg-file-system-access": "^2020.9.5",
    "concurrently": "^7.2.2",
    "msw": "^0.44.2",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "msw": {
    "workerDirectory": "public"
  }
}
