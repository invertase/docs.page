{
  "name": "oss-docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:prod": "cross-env NODE_ENV=production next dev",
    "build": "npm run clean && next build",
    "start": "next start",
    "serve": "npm run build && npm run start",
    "clean": "rimraf .next",
    "test": "jest",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "check:linting": "eslint . --max-warnings=0",
    "check:formatting": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "check:typescript": "tsc",
    "check:spelling": "spellchecker --quiet --files=\"docs/**/*.{md,mdx}\" --dictionaries=\"./dictionary.js\" --reports=\"spelling.json\" --plugins spell indefinite-article repeated-words syntax-mentions syntax-urls frontmatter"
  },
  "dependencies": {
    "@docsearch/react": "^3.0.0-alpha.32",
    "@invertase/next-mdx-remote": "^1.0.0",
    "@octokit/graphql": "^4.5.7",
    "@react-hook/window-scroll": "^1.3.0",
    "@tailwindcss/typography": "^0.4.1",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "a2a": "^0.2.0",
    "ansi-to-html": "^0.6.14",
    "autoprefixer": "^10.2.6",
    "classnames": "^2.2.6",
    "color": "^3.1.3",
    "gray-matter": "^4.0.2",
    "hast-util-has-property": "^2.0.0",
    "hast-util-heading-rank": "^2.1.0",
    "hast-util-parse-selector": "^3.1.0",
    "hast-util-select": "^5.0.0",
    "hast-util-to-text": "^3.0.0",
    "highlight.js": "^11.0.1",
    "is-html": "^2.0.0",
    "isomorphic-dompurify": "^0.9.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.get": "^4.4.2",
    "next": "^11.0.0",
    "next-transpile-modules": "^8.0.0",
    "nprogress": "^0.2.0",
    "parse-numeric-range": "^1.2.0",
    "postcss": "^8.1.7",
    "react": "17.0.1",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "17.0.1",
    "react-medium-image-zoom": "^4.3.1",
    "react-scrollspy": "^3.4.3",
    "react-test-renderer": "^17.0.1",
    "refractor": "^3.2.0",
    "rehype-accessible-emojis": "^0.3.2",
    "rehype-highlight": "^4.1.0",
    "rehype-slug": "^4.0.1",
    "remark-unwrap-images": "^2.1.0",
    "rimraf": "^3.0.2",
    "sanitize-html": "^2.2.0",
    "serialize-error": "^7.0.1",
    "stacktracey": "^2.0.15",
    "tailwindcss": "^2.1.4",
    "unist-util-visit": "^2.0.3",
    "use-dark-mode": "^2.3.1",
    "use-onclickoutside": "^0.3.1",
    "valid-data-url": "^3.0.1"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/color": "^3.0.1",
    "@types/jest": "^26.0.15",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/lodash.get": "^4.4.6",
    "@types/node": "^14.14.7",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.11",
    "@types/react-copy-to-clipboard": "^5.0.0",
    "@types/react-scrollspy": "^3.3.3",
    "@types/valid-data-url": "^2.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.28.0",
    "eslint-config-next": "^11.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "jest": "^27.0.4",
    "prettier": "2.3.1",
    "spellchecker-cli": "^4.4.0",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.2"
  }
