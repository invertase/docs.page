{
  "name": "oss-docs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"PORT=3000 next dev\" \"yarn express-bundler\"",
    "express-bundler": "cd bundler && yarn dev",
    "dev:prod": "cross-env NODE_ENV=production next dev",
    "build": "npm run clean && next build",
    "start": "next start",
    "serve": "npm run build && npm run start",
    "clean": "rimraf .next",
    "test": "jest",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "check:linting": "eslint . --max-warnings=0",
    "check:formatting": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,mdx}\"",
    "check:typescript": "tsc",
    "check:spelling": "spellchecker --quiet --files=\"docs/**/*.{md,mdx}\" --dictionaries=\"./dictionary.js\" --reports=\"spelling.json\" --plugins spell indefinite-article repeated-words syntax-mentions syntax-urls frontmatter",
    "postinstall": "patch-package && node scripts/copy-shiki-assets.js"
  },
  "dependencies": {
    "@docsearch/react": "^3.0.0-alpha.32",
    "@octokit/graphql": "^4.5.7",
    "@react-hook/window-scroll": "^1.3.0",
    "@tailwindcss/typography": "^0.4.1",
    "a2a": "^0.2.0",
    "ansi-to-html": "^0.7.1",
    "autoprefixer": "^10.3.6",
    "axios": "^0.21.4",
    "classnames": "^2.2.6",
    "color": "^4.0.1",
    "esbuild": "^0.13.2",
    "gray-matter": "^4.0.2",
    "hast-util-has-property": "^2.0.0",
    "hast-util-heading-rank": "^2.1.0",
    "hast-util-parse-selector": "^3.1.0",
    "hast-util-select": "^5.0.0",
    "lodash.get": "^4.4.2",
    "lodash.union": "^4.6.0",
    "mdast-util-to-string": "^3.1.0",
    "mdx-bundler": "^6.0.1",
    "micromark-extension-mdx-jsx": "^1.0.0",
    "next": "^11.0.0",
    "next-transpile-modules": "^8.0.0",
    "nprogress": "^0.2.0",
    "parse-numeric-range": "^1.2.0",
    "patch-package": "^6.4.7",
    "postcss": "^8.3.8",
    "postinstall-postinstall": "^2.1.0",
    "probot": "^12.0.0",
    "react": "17.0.2",
    "react-copy-to-clipboard": "^5.0.2",
    "react-dom": "17.0.2",
    "react-medium-image-zoom": "^4.3.1",
    "react-scrollspy": "^3.4.3",
    "react-test-renderer": "^17.0.1",
    "rehype-accessible-emojis": "^0.3.2",
    "rehype-slug": "^5.0.0",
    "remark-gfm": "^2.0.0",
    "remark-unwrap-images": "^3.0.0",
    "rimraf": "^3.0.2",
    "serialize-error": "^8.1.0",
    "shiki": "^0.9.5",
    "stacktracey": "^2.0.15",
    "tailwindcss": "^2.2.16",
    "unist-util-visit": "^4.1.0",
    "valid-data-url": "^4.0.0"
  },
  "devDependencies": {
    "concurrently": "^6.4.0",
    "@types/classnames": "^2.2.11",
    "@types/color": "^3.0.1",
    "@types/jest": "^27.0.2",
    "@types/lodash.camelcase": "^4.3.6",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.union": "^4.6.6",
    "@types/node": "^16.10.1",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.11",
    "@types/react-copy-to-clipboard": "^5.0.0",
    "@types/react-scrollspy": "^3.3.3",
    "@types/sanitize-html": "^2.3.1",
    "@types/valid-data-url": "^2.0.0",
    "@types/wicg-file-system-access": "^2020.9.4",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.28.0",
    "eslint-config-next": "^11.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.24.0",
    "fs-extra": "^10.0.0",
    "jest": "^27.2.2",
    "prettier": "2.4.1",
    "spellchecker-cli": "^4.4.0",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.2"
  }
}
